{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "golf2",
      "request": "launch",
      "mainClass": "zfg.golf2",
      "projectName": "zfg",
      // "vmArgs": ["-XX:+UnlockDiagnosticVMOptions", "-Xlog:class+load=info", "-XX:+LogCompilation", "-XX:+PrintAssembly", "-XX:MaxInlineSize=0"]
      "vmArgs": ["-XX:MaxInlineSize=0"]
    },
    {
      "type": "java",
      "name": "golf",
      "request": "launch",
      "mainClass": "zfg.golf",
      "projectName": "zfg"
    },
    {
      "name": "Current Java File",
      "type": "java",
      "request": "launch",
      "mainClass": "${file}"
    },
    {
      "name": "Run Main",
      "type": "java",
      "request": "launch",
      "mainClass": "zfg.Main",
      "args": [
        "src/test/resources/test.zfg"
      ],
      "console": "externalTerminal"
    },
    {
      "name": "ANTLR Grammar",
      "type": "antlr-debug",
      "request": "launch",
      "grammar": "src/main/antlr/ZfgParser.g4",
      "startRule": "start",
      "input": "src/test/resources/test.zfg",
      "printParseTree": true,
      "visualParseTree": true
    }
  ]
}
