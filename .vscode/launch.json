{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "java",
      "name": "bench",
      "request": "launch",
      "mainClass": "golf.bench",
    },
    {
      "type": "java",
      "name": "golf2",
      "request": "launch",
      "mainClass": "zfg.golf2",
      "projectName": "zfg",
      "vmArgs": [
        "-XX:MaxInlineSize=0"
      ]
    },
    {
      "name": "Current Java File",
      "type": "java",
      "request": "launch",
      "mainClass": "${file}"
    },
    {
      "name": "Run Main",
      "type": "java",
      "request": "launch",
      "mainClass": "zfg.Main",
      "args": [
        "src/main/zfg/test.zfg"
      ],
      "console": "externalTerminal"
    },
    {
      "name": "ANTLR Grammar",
      "type": "antlr-debug",
      "request": "launch",
      "grammar": "src/main/antlr/ZfgParser.g4",
      "startRule": "start",
      "input": "src/main/zfg/types.zfg",
      "printParseTree": true,
      "visualParseTree": true
    }
  ]
}
