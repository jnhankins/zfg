


// Case #1: Forward declaration
let People: type = [Person]
let Person: type = (
  uid: u32,
  age: u08,
)

// Case #2: Self-circular reference
let Tree: type = (
  value: i32,
  children: [Tree]
)

// Case #3: Mutually-circular references
let A: type = (
  value: i32,
  array: [B]
)
let B: type = (
  value: i32,
  array: [A]
)
